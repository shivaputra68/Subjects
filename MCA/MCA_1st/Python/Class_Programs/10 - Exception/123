while 1:
    try:
        n1 = int(input("Enter the integer number1 --> "))
        n2 = int(input("Enter the integer number2 --> "))
        c = n1 / n2
        print ("Division of two number==> ",c)
        print ("-"*60)
    except ZeroDivisionError:
        print ("ZeroDivisionError Handled ")
    except ValueError:
        print ("ValueError Handled ")
    







